<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="net.skhu.example">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".firebase.recyclerView.MainActivity">
        </activity>
        <activity android:name=".firebase.logIn.MainActivity">
        </activity>
        <activity android:name=".listView.MainActivity">
        </activity>
        <activity
                android:name=".alertDialog.MainActivity"
                android:label="Alerts">
        </activity>
        <activity android:name=".spinner.MainActivity">
        </activity>
        <activity android:name=".checkbox.MainActivity">
        </activity>
        <activity android:name="net.skhu.example.firebase.sample.MainActivity">
        </activity>
    </application>

</manifest>